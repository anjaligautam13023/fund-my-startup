        <div class="container">
          <div class="heading_container">
            <h2 style="text-align: center;">
                Add your product
            </h2>
          </div>
        </div>
       
     
   <br>
   <br>
   <form method="post" action="/about/form-submit" enctype="multipart/form-data">
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="pname">product name</label>
        <input type="text" class="form-control" id="pname" placeholder="product name" name="pname" required>
      </div>
      <div class="form-group col-md-6">
        <label for="date">launch date</label>
        <input type="date" class="form-control" id="date" placeholder="launch date" name="ldate" required>
      </div>
    </div>
    <div class="form-group">
      <label for="founder">other founder</label>
      <input type="text" class="form-control" id="founder" placeholder="enter , separated emails" name="founder" required>
    </div>
    <div class="form-group">
      <label for="description"> description </label>
      <textarea type="text" class="form-control" id="description" rows="5"  cols="5" placeholder="Add description of project" name="description"></textarea>
    </div>
    <div class="form-row" >
      <div class="form-group col-md-6">
        <label for="cv">Current valuation</label>
        <input type="number" class="form-control" id="cv" name="cv" required>
      </div>
    </div>
    <div class="form-group" >
        <label for="photo">upload photo</label>
        <input type="file" class="form-control-file" id="photo"  accept="image/png, image/jpeg" name="photo" required>
      </div>
    <button type="submit" class="submit">submit</button>
    
  </form>
    </div>
